    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host=0.0.0.0",
                "--port=5000"
            ],
            "jinja": true,
            "autoStartBrowser": false ,
            "python": "f:/Programming training/backtesting/.venv/Scripts/python.exe"
        },
        {
            "name" : "Python current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "f:/Programming training/backtesting/.venv/Scripts/python.exe"
        }
    ]
}